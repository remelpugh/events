{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "moduleResolution": "node",
        "inlineSourceMap": false,
        "inlineSources": false,
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmitOnError": false,
        "noLib": false,
        "noResolve": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "EventArgs.ts",
        "Events.ts",
        "IEventArgs.ts",
        "ISubscriber.ts",
        "ISubscriberOptions.ts",
        "ISubscription.ts"
    ],
    "exclude": []
}
